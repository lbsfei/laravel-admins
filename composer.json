{
    "name": "feiger/laravel-admins",
    "description": "Laravel-admin 多后台的工具",
    "type": "library",
    "keywords": ["laravel-admins", "multi-admin"],
    "homepage": "https://github.com/lbsfei/laravel-admins",
    "license": "MIT",
    "version": "v1.0.0",
    "authors": [
        {
            "name": "feiger",
            "email": "351275698@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
